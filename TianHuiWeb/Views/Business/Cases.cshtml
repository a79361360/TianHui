
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
    <meta name="format-detection" content="telephone=no">
    <title>天汇传媒案例与作品</title>
    <link rel="stylesheet" href="../assets/static/css/default.css?v=142">
    <script src="../assets/static/js/jquery-1.11.1.min.js"></script>
    <script src="../assets/static/js/fun.js?v=141"></script>
    <link rel="stylesheet" href="../assets/static/css/master.css">
    <link rel="stylesheet" href="../assets/static/js/respond.src.js">
</head>
<body class="case-page">
    <!-- top start -->
    @Html.Action("HomePartial", "Home", new { chanelid = 9 })
    <!-- end top -->
    <div class="container">
        @Html.Raw(ViewBag.banner)
        <div class="all case mt30">
            @Html.Raw(ViewBag.title)
            <ul class="case-index-list fn-clear">
                @Html.Raw(ViewBag.Case1)
            </ul>
        </div>
        <div class="case-index-title-m">获奖作品</div>
        <ul class="case-index-award">
            @Html.Raw(ViewBag.Case2)
        </ul>
    </div>
    <!-- foot start -->
    @Html.Partial("~/Views/Shared/Foot.cshtml")
    <!-- end foot -->
</body>
</html>
